<% if(messages.error) { %>
    <div class="alert alert-danger">
        <%= messages.error %>
    </div>
<% } %>

<% if(error_msg != "") { %>
    <div class="alert alert-danger">
        <%= error_msg %>
    </div>
<%}%>

<% if(success_msg != "") { %>
    <div class="alert alert-success">
        <%= success_msg %>
    </div>
<%}%>